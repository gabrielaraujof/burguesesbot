{
  "name": "burguesesbot",
  "version": "2.0.0",
  "description": "Telegram Bot for burgueses group",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "develop": "node --import @swc-node/register/esm-register ./src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "lint": "echo lint",
    "build": "swc ./src -q -d dist --strip-leading-paths"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielaraujof/burguesesbot.git"
  },
  "keywords": [
    "telegram",
    "bot"
  ],
  "author": "Gabriel Araujo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabrielaraujof/burguesesbot/issues"
  },
  "homepage": "https://github.com/gabrielaraujof/burguesesbot#readme",
  "devDependencies": {
    "@swc-node/register": "^1.11.1",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@types/aws-lambda": "^8.10.152",
    "@types/node": "^24.5.0",
    "@vitest/coverage-v8": "^3.2.4",
    "aws-lambda": "^1.0.7",
    "nodemon": "^3.1.10",
    "serverless": "^4.18.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "discord.js": "^14.22.1",
    "dotenv": "^16.6.1",
    "ky": "^1.10.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-http": "^3.2.0",
    "telegraf": "^4.16.3"
  }
}
